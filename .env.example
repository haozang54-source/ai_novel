# LLM配置示例文件
# 复制此文件为 .env 并填入实际配置

# OpenAI兼容接口配置
OPENAI_API_BASE=http://taiji-stream-server-online-openapi.turbotke.production.polaris:8080/openapi/
OPENAI_API_KEY=your_api_key_here
OPENAI_MODEL=tcop_ai_qwen3_235B_dingli

# Ollama配置（可选）
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=qwen2.5:7b-instruct

# LLM提供商选择: openai 或 ollama
LLM_PROVIDER=openai

# LLM参数
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=3000
